{
  "name": "dev-quotes",
  "version": "1.0.0",
  "description": "Wallpapers with developer quotes.",
  "main": "start.js",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12",
    "yarn": ">=1.22"
  },
  "scripts": {
    "check-formatting": "prettier --check \"**/*.{css,html,js,json,md,scss,ts,tsx,yml}\"",
    "check-types": "tsc",
    "start": "yarn install && ts-node src/generate-wallpapers.ts",
    "update-dependencies": "yarn upgrade-interactive && rm yarn.lock && yarn install"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.10.13",
    "adm-zip": "^0.5.16",
    "prettier": "^3.8.1",
    "puppeteer": "^24.37.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
